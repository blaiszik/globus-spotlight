

  <div class="list-group" id="selectable">
        <% for(var i=0; i<hits.hits.length; i++) {%>
        <div id="result-set-<%= hits.hits[i]._id %>" class="list-group-item result-set-item primary">

            <div class="pull-right">
            <div><label class='label-endpoint'><%= hits.hits[i]._source.endpoint %></label></div> <div><label class='label-last-modified'> <%= hits.hits[i]._source.last_modified%></label></div></div>
            <div><h4><%= hits.hits[i]._source.name %> - <%= hits.hits[i]._source.size %>
            
        	</h4></div>
            <div id="result-set-tag-<%= hits.hits[i]._id %>">
            <%if(hits.hits[i]._type=='catalog'){%>
        		<label class="glyphicon glyphicon-book">
        		</label>
        	<%}else{%>
        		<label class="glyphicon glyphicon-cloud">
        		</label>
        	<%}%>
            	<% if(tags = hits.hits[i]._source.tags){%>
            		<% for(var j=0; j<tags.length; j++) { %>
            			<label class="label label-primary"> <%= tags[j] %> </label>&nbsp 
            		<% } %>
            	<%}%>
            </div>
        </div>
        <% } %>
    </div> 